{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ]
  }
}